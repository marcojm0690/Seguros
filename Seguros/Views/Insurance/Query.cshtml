@model IEnumerable<Seguros.DAL.Models.Insurance>
<div class="jumbotron">
    <h1>Consultar Seguro</h1>
</div>
<div class="row">
    <table class="table table-dark">
        <tr>
            <th scope="col">
                Insurance
            </th>
            <th scope="col">
                Description
            </th>
            <th scope="col">
                Name
            </th>
            <th scope="col">
                Risk Type
            </th>
        </tr>
        @foreach (var insurance in Model)
        {
        <tr>
            <td scope="row">
                @insurance.Name
            </td>
            <td>
                @insurance.Description
            </td>
            <td>
                @insurance.Client.Name
            </td>
            <td>
                @insurance.RiskType.Name
            </td>
        </tr>
        }
    </table>
    <p>

    </p>
</div>
